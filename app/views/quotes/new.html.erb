<div class="booyah-box col-10 offset-1">
<h4> Submit a  Quote </h4>
</div>


<%= simple_form_for @quote do |f| %>
    <%= f.input :name %>
    <%= f.input :email %>
    <%= f.input :phone_number %>
    <%= f.input :destination %>
    <%= f.input :description %>
    <%= f.submit "Submit" %>
<%  end %>